<section class="hobbydom">
    <div class="hobydom__wrap">
        <div class="hobbydom-left">
            <img src="images/hobbydom.svg" alt="[[%hobbydom_svg_alt? &topic=`hobbydom` &namespace=`hobbydom` ]]">
            <div id="wrap">
                <div class="eye">
                    <div class="iris"></div>
                </div>
                <div class="eye eye-right">
                    <div class="iris"></div>
                </div>
            </div>
        </div>
        <div class="hobbydom-right">
            <div class="hobbydom__title--wpapper">
                <h2>[[%hobbydom_title_wpapper? &topic=`hobbydom` &namespace=`hobbydom` ]]</h2>
            </div>
            <div class="hobbydom__icons">
                <div class="hobbydom__icon__wrapper">
                    <img src="images/house.svg" alt="">
                    [[%hobbydom_icon_wrapper1? &topic=`hobbydom` &namespace=`hobbydom`]]

                </div>
                <div class="hobbydom__icon__wrapper">
                    <img src="images/sun.svg" alt="">
                    [[%hobbydom_icon_wrapper2? &topic=`hobbydom` &namespace=`hobbydom`]]

                </div>
                <div class="hobbydom__icon__wrapper">
                    <img src="images/blanket.svg" alt="">
                    [[%hobbydom_icon_wrapper3? &topic=`hobbydom` &namespace=`hobbydom`]]

                </div>
            </div>
        </div>
    </div>
</section>